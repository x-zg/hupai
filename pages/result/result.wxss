.container {
  padding: 20rpx;
}
.title {
  font-size: 36rpx;
  font-weight: bold;
  text-align: center;
  margin-bottom: 50rpx;
}
.winner {
  font-size: 32rpx;
  text-align: center;
  margin-bottom: 30rpx;
}
.score-list {
  width: 10%;
  margin-bottom: 50rpx;
}
.score-item {
  display: flex;
  justify-content: space-between;
  padding: 20rpx 0;
  border-bottom: 2rpx solid #cccccc;
}
button {
  background-color: #1aad19;
  color: #ffffff;
  padding: 20rpx 40rpx;
  border-radius: 10rpx;
}
